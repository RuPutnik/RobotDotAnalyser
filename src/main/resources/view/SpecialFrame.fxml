<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.putnik.robotdotanalyser.SpecialFrame">
   <children>
      <HBox alignment="CENTER" prefHeight="58.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Button fx:id="startPrint" mnemonicParsing="false" text="Запуск" />
            <Button fx:id="moveCursor" mnemonicParsing="false" text="Переместить курсор" />
         </children>
      </HBox>
      <Label text="Координаты курсора (x:y)" />
      <HBox prefHeight="23.0" prefWidth="200.0" spacing="10.0">
         <children>
            <TextField fx:id="coordinatesField" prefHeight="25.0" prefWidth="165.0" />
            <Pane fx:id="colorPanel" prefHeight="13.0" prefWidth="26.0" />
         </children>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
